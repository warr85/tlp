<?php


namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * CursoClase
 *
 * @ORM\Table(name="curso_clase", uniqueConstraints={@ORM\UniqueConstraint(name="i_curso_clase", columns={"id_curso"})}, indexes={@ORM\Index(name="fki_curso_clase", columns={"id_curso"}), @ORM\Index(name="IDX_A893E1FC3885FA9C", columns={"id_curso"})})
 * @ORM\Entity
 */
class CursoClase
{
    /**
     * @var \AppBundle\Entity\Curso
     *
     * @ORM\ManyToOne(targetEntity="AppBundle\Entity\Curso")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="id_curso", referencedColumnName="id", nullable=false)
     * })
     */
    private $idCurso;
    
    
    /**
     * @var string
     *
     * @ORM\Column(name="codigo", type="string", length=50, nullable=false, options={"comment" = "codigo de la clase"})
     */
    private $codigo;
    
    
    /**
     * @var string
     *
     * @ORM\Column(name="nombre", type="string", length=250, nullable=true, options={"comment" = "Nombre opcional para el salÃ³n de clase"})
     */
    private $nombre;
         
    
    /**
     * @var integer
     *
     * @ORM\Column(name="cupo", type="integer", nullable=false, options={"comment" = "cupo del curso"})
     */
    private $cupo;
    
    /**
     * @var float
     *
     * @ORM\Column(name="precio", type="float", nullable=false, options={"comment" = "Valor del modulo"})
     */
    private $precio;
    
    
    /**
     * @var boolean
     *
     * @ORM\Column(name="is_visible", type="boolean", nullable=false, options={"comment" = "Esta disponible el curso"})
     */
    private $isVisible;
    
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false, options={"comment" = "Identificador de la clase"})
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     * @ORM\SequenceGenerator(sequenceName="estatus_id_seq", allocationSize=1, initialValue=1)
     */
    private $id;
    
    /** @ORM\Column(name="fecha_inicio", type="datetime", nullable=false, options={"comment" = "Fecha de creaciÃ³n de la solicitud"})
    
    */
    
    private $fechaInicio;
    
    
    /** @ORM\Column(name="fecha_fin", type="datetime", nullable=false, options={"comment" = "Fecha de actualizacion de la solicitud"})
    
    */
    
    private $fechaFin;
    
    

    

    /**
     * Set codigo
     *
     * @param string $codigo
     * @return CursoClase
     */
    public function setCodigo($codigo)
    {
        $this->codigo = $codigo;

        return $this;
    }

    /**
     * Get codigo
     *
     * @return string 
     */
    public function getCodigo()
    {
        return $this->codigo;
    }

    /**
     * Set nombre
     *
     * @param string $nombre
     * @return CursoClase
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;

        return $this;
    }

    /**
     * Get nombre
     *
     * @return string 
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set cupo
     *
     * @param integer $cupo
     * @return CursoClase
     */
    public function setCupo($cupo)
    {
        $this->cupo = $cupo;

        return $this;
    }

    /**
     * Get cupo
     *
     * @return integer 
     */
    public function getCupo()
    {
        return $this->cupo;
    }

    /**
     * Set precio
     *
     * @param float $precio
     * @return CursoClase
     */
    public function setPrecio($precio)
    {
        $this->precio = $precio;

        return $this;
    }

    /**
     * Get precio
     *
     * @return float 
     */
    public function getPrecio()
    {
        return $this->precio;
    }

    /**
     * Set isVisible
     *
     * @param boolean $isVisible
     * @return CursoClase
     */
    public function setIsVisible($isVisible)
    {
        $this->isVisible = $isVisible;

        return $this;
    }

    /**
     * Get isVisible
     *
     * @return boolean 
     */
    public function getIsVisible()
    {
        return $this->isVisible;
    }

    /**
     * Set idCurso
     *
     * @param \AppBundle\Entity\Curso $idCurso
     * @return CursoClase
     */
    public function setIdCurso(\AppBundle\Entity\Curso $idCurso)
    {
        $this->idCurso = $idCurso;

        return $this;
    }

    /**
     * Get idCurso
     *
     * @return \AppBundle\Entity\Curso 
     */
    public function getIdCurso()
    {
        return $this->idCurso;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
