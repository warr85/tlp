{% extends 'base_admin.html.twig' %}

{% block stylesheets %} 
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('assets/vendor/terminal/jquery.terminal.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/vendor/FileTree/css/file-tree.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/AppBundle/css/bombillo.css') }}">
